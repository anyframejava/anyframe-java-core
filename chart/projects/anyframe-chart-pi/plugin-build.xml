<?xml version="1.0" encoding="UTF-8" ?>
<plugin name="chart" description="Chart Plugin consists of functions that provides fusionchart-free support with JAXB Model class, and reference libraries necessary to use these open sources.">
  <groupId>org.anyframe.plugin</groupId>
  <artifactId>anyframe-chart-pi</artifactId>
  <version>1.0.3</version>
  <dependent-plugins>
    <dependent-plugin name="core" version="2.0.0&gt;*&gt;1.4.0"/>
  </dependent-plugins>
  <resources>
    <resource dir="src/main/resources" filtered="true" packaged="false">
      <include name="**/*chart*.xml"/>
    </resource>
    <resource dir="src/main/webapp" filtered="false" packaged="false">
      <include name="**/chart/**/*.*"/>
    </resource>
  </resources>
  <samples>true</samples>
  <interceptor>
    <class></class>
  </interceptor>
  <essential>false</essential>
</plugin>